<div class="container childcontent ">
    <div class="panel panelInquiryControls panel-primary panelWidth PanelHeight">
        <!--style=" background:url(../Images/BackTransparrent.png)no-repeat   center center fixed;-webkit-background-size: cover;-moz-background-size: cover;-o-background-size: cover;background-size: cover;"> -->
        <div class="panel-heading PanelTitle">
            <h3 class="panel-title pull-left ">
            </h3>
        </div>
        <div class="panel-body" style="padding-top: 60px;">
            <div class="row form-inline">
                @*<div id="btnstockInq" class="col-lg-1 col-md-12 col-sm-12 col-xs-12">
                </div>*@
                @if (PDXWEB.BusinessModels.Common.UserRights.hasRight("Z01S110_11"))
                {
                    <div id="btnstockInq" class="col-lg-3 col-md-3 col-sm-6 col-xs-6 SoOrder">
                        <div class="DivStyle">
                            <div style="padding-bottom: 43px;">
                                <img class="ImgStyle" src="../Images/Icons/StockInquiry.png" />
                            </div>
                        </div>
                        <div class="divp">
                            <p class="Pstyle">@Resources.Resource.StockInquiry</p>
                        </div>
                        @*#23527C;*@
                    </div>
             
             
                }
                @if (PDXWEB.BusinessModels.Common.UserRights.hasRight("A01S020_02"))
                {
                    <div id="btnQutn" class="col-lg-3 col-md-3 col-sm-6 col-xs-6 SoOrder">
                        <div id="btnQutn" class="DivStyle">
                            <div style="padding-bottom: 43px;">
                                <img class="ImgStyle" src="../Images/Icons/GenerateQuotation.png" />
                            </div>
                        </div>
                        <div class="divp">
                            <p class="Pstyle">@Resources.Resource.GenerateQuotation</p>
                        </div>
                    </div>
                }
                @if (PDXWEB.BusinessModels.Common.UserRights.hasRight("Z01S100_12"))
                {  
                    <div id="OStat" class="col-lg-3 col-md-3 col-sm-6 col-xs-6 SoOrder">
                        <div id="OStat" class="DivStyle">
                            <div style="padding-bottom: 43px;">
                                <img class="ImgStyle" src="../Images/Icons/ConverttoOrder.png" />
                            </div>
                        </div>
                        <div class="divp">
                            <p class="Pstyle">
                                Parts Information
                            </p>
                        </div>
                    </div>
              
                }
                @if (PDXWEB.BusinessModels.Common.UserRights.hasRight("S01S080_04"))
                {

                    <div id="btnCTO" class="col-lg-3 col-md-3 col-sm-6 col-xs-6 SoOrder">
                        <div id="btnCTO" class="DivStyle">
                            <div style="padding-bottom: 43px;">
                                <img class="ImgStyle" src="../Images/Icons/OrderStatus.png" />
                            </div>
                        </div>
                        <div class="divp">
                            <p class="Pstyle">
                                @Resources.Resource.OrderStatus
                            </p>
                        </div>
                    </div>
                }
                @if (PDXWEB.BusinessModels.Common.UserRights.hasRight("S04S010_01"))
                {

                    <div id="btnSalesInvoice" class="col-lg-3 col-md-3 col-sm-6 col-xs-6 SoOrder">
                        <div id="btnCTO" class="DivStyle">
                            <div style="padding-bottom: 43px;">
                                <img class="ImgStyle" src="../Images/Icons/Invoice.png" />
                            </div>
                        </div>
                        <div class="divp">
                            <p class="Pstyle">
                                @Resources.Resource.SalesInvoice
                            </p>
                        </div>
                    </div>
                }
                @if (PDXWEB.BusinessModels.Common.UserRights.hasRight("M01S010_01"))
                {
                    <div id="btnPriceLists" class="col-lg-3 col-md-3 col-sm-6 col-xs-6 SoOrder">
                        <div id="btnPriceList" class="DivStyle">
                            <div style="padding-bottom: 43px;">
                                <img class="ImgStyle" src="../Images/Icons/ListPice.png" />
                            </div>
                        </div>
                        <div class="divp">
                            <p class="Pstyle">
                                @Resources.Resource.ListPrice
                            </p>
                        </div>
