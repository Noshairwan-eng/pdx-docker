           @* @if (PDXWEB.BusinessModels.Common.UserRights.hasRight("M01S010_01"))//hst001 Check right before rendering sales price list
            { 
                <div style="margin-top: 70px;">
                    <div>
                        <img alt="Download"src="@Url.Content("~/images/File-Download.png")"   style="width: 19px; height: 19px ! important; padding: 0px 3px 3px 0px ! important; margin: -3px -6px 1px 1px ! important;" />
                        <a id="lnkDownloadPriceList" href="#" style="font-weight: 600; color: red; font-size: 15px;
                            padding: 11px 4px 4px 5px ! important;">@Resources.Resource.DownloadPriceList
                        </a>
                    </div>
                    <div>
                    </div>
                </div>
            }*@
        </div>
    </div>
</div>
<div class="hiddenForm" style="display: none !important;">
    <form id="frmDownloadPriceList" method="post" action="@Url.Action("DownloadPriceList", "Home", null)" >
    </form>
</div>
<div class="modal fade" id="MessageModal" role="dialog" style="padding-top: 169px;">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    &times;</button>
                <h4 class="modal-title">
                    @Resources.Resource.DownloadPriceList</h4>
            </div>
            <div class="modal-body" id="modalBody">
                <img class="imgError" />
                <label id="lblMessageText" class="lblError">
                </label>
            </div>
            <div class="modal-footer">
                <button style="text-align: center; margin-left: auto !important; float: none !important;"
                    id="btnOk" class="btn btn-default center-block" data-dismiss="modal">
                    @Resources.Resource.btnOK
                </button>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">

       $(document).ready(function () {
         $(".panelInquiryControls").parent("div").prepend("<div id='divLoading'></div>");    
          
           $("#lnkDownloadPriceList").on("click",function (e) {
            $.post("@Url.Action("DownloadPriceList", "Home", null)",null,function(data){
             if (CheckForSessionExpiry(data))
            {
            SessionExpiredRedirect();
            }         
            else
             {
            
            $(".hiddenForm").html(data);
            
            }
            
            });
             
            e.preventDefault();
           });


           $("#btnstockInq").click(function () {
             window.location.href="@Url.Action("StockInquiry", "StockInquiry", null)";
           });

           $("#btnCTO").click(function () {
            window.location.href="@Url.Action("SOStatusInquiryByOrderNo", "SOStatusInquiry", null)";
            });
           $("#OStat").click(function () {
             window.location.href="@Url.Action("PartsMasterInquiry", "StockInquiry", null)";
           });
           $("#btnQutn").click(function () {
             window.location.href="@Url.Action("Index", "QuotationCondition", null)";
           });
            $("#btnSalesInvoice").click(function () {
             window.location.href="@Url.Action("SalesInvoiceInquiry", "SalesInvoiceInquiry", null)";
           });
           $("#btnPriceLists").click(function(){
               window.location.href = "@Url.Action("Index","ListPrice", null)"
           });


       });
</script>
<script type="text/javascript">

    $(document).ready(function () {

        $(".DivStyle").hover(function () {
            // $(this).addClass("DivStyle_hover");
            //               $(this).next("div").removeClass("divp");
            $(this).addClass("Pstyle_hover");

        },
           function () {
               // $(this).removeClass("DivStyle_hover");
               // $("p.Pstyle").removeClass("Pstyle_hover");
               $(this).removeClass("Pstyle_hover");
               //  $(this).next("div").addClass("divp");

           });
        $(".divp").hover(function () {
            // $(this).addClass("DivStyle_hover");
            $(this).prev("div").addClass("Pstyle_hover");
            //               $(this).removeClass("divp");
            //               $(this).addClass("Pstyle_hover");

        },
           function () {
               // $(this).removeClass("DivStyle_hover");
               // $("p.Pstyle").removeClass("Pstyle_hover");
               $(this).prev("div").removeClass("Pstyle_hover");
               //               $(this).removeClass("Pstyle_hover");
               //               $(this).addClass("divp");

           });

    });  //doc.ready end
    $(document).ajaxStart(function () {

        $("div#divLoading").addClass('show');

    });
    $(document).ajaxStop(function () {
        $("div#divLoading").removeClass('show');
    });

    $(document).ajaxError(function () {
        $("div#divLoading").removeClass('show');
    });

</script>
