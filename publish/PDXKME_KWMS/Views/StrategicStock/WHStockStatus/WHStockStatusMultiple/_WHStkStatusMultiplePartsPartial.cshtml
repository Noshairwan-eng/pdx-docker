@{
    ViewBag.Title = "_WHStkStatusMultiplePartsPartial";
}

 @if (true)
 {
     string str = "";
 }

@if (ViewData.Model != null)
{
    
  <div id="main" class="datagrid table-responsive" >
  <table class="table table-striped table-hover" id="tblWH">
 <thead>
 <tr class="gridHead">
 @foreach (var item in ViewBag.Columns)
 {
     if (item.ToString() == "isChain")
     {
     <th style="display:none;">
     @item.ToString()
     </th>
     }
     else
     {
     <th>
     @item.ToString()
     </th>
     }
 }
</tr>


 </thead>
 <tbody>
 
@foreach (var item in ViewData.Model)
{
<tr class="gridRow"> 
 @for (int i = 0; i < Enumerable.Count(ViewBag.Columns); i++)
 {
     if (i == 0)
     {
           <td class="@(item[0].ToString() == "0" ? "ChainedPart" : "")" style="display:none;">@item[0]</td>
     }
     else 
     {
     <td> @item[i].ToString() </td>
     }
 }


 </tr>   
}


 </tbody>
</table>
    
    
    </div>
    
}
 

  <script type="text/javascript">

      $("#tblWH tbody tr").each(function () { //loop over each row
          console.log($(this).find('td').eq(0).text());
          if ($.trim($(this).find('td').eq(0).text()) == "0") {
              $(this).addClass("ChainedPart").addClass("chainParent");
          }
      });

      $(document).ready(function () {
          var Message = "@ViewBag.Message";
          if (Message != "" && Message != null) {           
             // $("#MessageModalBody").html("");            
             // $("#MessageModalBody").prepend(Message).attr("style", "color:red !important");
             // $("#MessageModal").modal();
              txtError = Message;
              txtErrorType = "0";
              DisplayModal(txtErrorType, txtError);
          }
      });   //

  </script>




