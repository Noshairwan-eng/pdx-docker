<?xml version="1.0" encoding="utf-8"?>
<settings>
  <theme>
    <predefined>true</predefined>
  </theme>
  <!-- Section with general text messages. -->
  <strings>
    <!-- Text of the label in popup message shown on loading a topic. -->
    <topicSpinnerText>In caricamento...</topicSpinnerText>
  </strings>
  <!-- Section with text that is shown in the topic area. -->
  <topic>
    <strings>
      <!-- This message is shown when topic cannot be loaded. -->
      <notfoundText>Argomento non trovato</notfoundText>
      <!-- This window title is shown when topic cannot be loaded. -->
      <notfoundTitle>Argomento non trovato</notfoundTitle>
    </strings>
  </topic>
  <!-- Section with text that is shown in the Index page. -->
  <index>
    <strings>
      <!-- Tooltip for the Index text box. -->
      <filterTooltip>Filtra per parola chiave</filterTooltip>
      <!-- This message is shown when keywords aren't found in Index. -->
      <notfound>Nessuna parola chiave trovata.</notfound>
      <!-- This message is shown when one or more keywords are found. This message can contain a #{count} macro that is replaced with a found keywords number. -->
      <found>#{count} Parole chiave trovate.</found>
      <!-- Text of the "more" link in keyword list. -->
      <more>Ancora...</more>
    </strings>
  </index>
  <!-- Section with text that is shown in the Search page. -->
  <search>
    <!-- Buttons used on the search page. -->
    <buttons>
      <!-- Go button that starts search. -->
      <go>
        <!-- Text (or tooltip) for the Go button to start search. -->
        <label>Ricerca</label>
      </go>
    </buttons>
    <strings>
      <!-- Tooltip for the Search text box. -->
      <filterTooltip>Ricerca argomento</filterTooltip>
      <!-- This text will be shown in a popup when the user clicks the "Help" button on the search tab. -->
      <helpMessage>Puoi effettuare operazioni base nella stringa di ricerca #{and}, #{or}, #{not}. Esempi: football #{or} hockey, sports #{and} #{not} baseball</helpMessage>
      <!-- This message is shown in the search results area while data is loading. -->
      <loading>Motore di ricerca in caricamento...</loading>
      <!-- This message is shown if data fails to load. -->
      <loaderror>Errore: Errore di caricamento del motore di ricerca.</loaderror>
      <!-- This message is shown when search is disabled. -->
      <disabled>Il tasto ricerca e'disabilitato.</disabled>
      <!-- This message is shown when no topics are found. -->
      <notfound>Nessun argomento trovato.</notfound>
      <!-- This message is shown when topics are found. This message can contain a #{count} macro that is replaced with a found topics number. -->
      <found>#{count} Argomenti trovati.</found>
      <!-- This message is shown when search engine suggests a search string correction to the user. This message can contain a #{query} macro that is replaced with a link to the corrected search string (default: "Did you mean: #{query}") -->
      <correcting>Intendevi forse: #{query}</correcting>
      <!-- Text of the "more" link in search results list. -->
      <more>Ancora...</more>
    </strings>
    <!-- These strings define logical operators for search. -->
    <operators>
      <!-- The logical operator AND. -->
      <and>AND</and>
      <!-- The logical operator OR. -->
      <or>OR</or>
      <!-- The logical operator NOT. -->
      <not>NOT</not>
    </operators>
  </search>
  <!-- Text (or tooltips) for buttons. -->
  <buttons>
    <toc>
      <!-- Text (or tooltips) for the Contents button in the header. -->
      <label>Contenuto</label>
    </toc>
    <index>
      <!-- Text (or tooltips) for the Index button in the header. -->
      <label>Indice</label>
    </index>
    <prev>
      <!-- Text (or tooltips) for the Previous button at the top of the topic area. -->
      <label>Precedente</label>
    </prev>
    <next>
      <!-- Text (or tooltips) for the Next button at the top of the topic area. -->
      <label>Promissmo</label>
    </next>
    <prevBottom>
      <!-- Text (or tooltips) for the Previous button at the bottom of the topic area. -->
      <label>Precedente</label>
    </prevBottom>
    <nextBottom>
      <!-- Text (or tooltips) for the Next button at the bottom of the topic area. -->
      <label>Promissmo</label>
    </nextBottom>
  </buttons>
  <!-- Strings shown only if Accessibility Mode is set to Section 508. -->
  <accessibility>
    <!-- The title for icons in the table of contents. -->
    <toc>
      <!-- The title for closed books without topic. -->
      <closedBook>Annotazione chiusa senza argomento</closedBook>
      <!-- The title for closed books with topic. -->
      <closedBookTopic>Annotazione chiusa con argomento</closedBookTopic>
      <!-- The title for topic items. -->
      <topic>Argomento</topic>
    </toc>
    <!-- The title for collapsible section icon. -->
    <collapsibleSections>
      <!-- The title for a section in collapsed state. -->
      <collapsed>Clicca per espandere</collapsed>
      <!-- The title for a section in expanded state. -->
      <expanded>Clicca per ridurre</expanded>
    </collapsibleSections>
    <!-- List header for the list of topics of a keyword or group link (if there is more than one topic). This string can contain "#{count}" macro. -->
    <akLinksMenuHeader>#{count} argomenti trovati</akLinksMenuHeader>
  </accessibility>
</settings>