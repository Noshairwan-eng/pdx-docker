       .nav, .navbar-nav, .navbar-header
    {
        position: relative;
        z-index: 10;
    }
    
     .navbar-nav > li > a
        {
            padding-top: 10px !important;
            padding-bottom: 10px !important;
        }
        .navbar
        {
            margin-bottom:2px !important;
            min-height:0px !important;
        }
        .navbar-default
        {
            font-size: 12px !important;
            font-weight: bold;
            color: Blue;
        }
        /*font size of main menu*/
        .navbar-default .navbar-nav > li > a
        {
            color: rgb(255,255,255) !important; 
        }
        .navbar-default .navbar-nav > li > a:hover
        {
           background-color: Red !important;
            color: White; 
        }
        /*To show dropdown menu on hove*/
        ul.nav li.dropdown:hover > ul.dropdown-menu
        {
            display: block;
        }
        
        /* To change dropdown menu background color */
        .navbar-nav > li > .dropdown-menu
        {
            background-color: #000099;
            margin-top: 0px;
        }
        /*.dropdown-menu>li:hover { background-color:red !important; }*/
        /* To change dropdown menu  color*/
        .dropdown-menu li > a
        {
            color: white;
            height: 25px;
            padding-top: 0px;
            margin-top: 0px;
        }
        /*Horizantal divider for menu*/
        .divider
        {
            height: 0px !important;
        }
        
        /* dropdown menu hover color*/
        .dropdown-menu > li > a:hover
        {
           background-color: #9DBDEE !important;
     color: #333 !important;
            background-image: none;
            background-repeat: repeat;
        }
         .dropdown-menu li > a
        {
            color: white !important;
            height: 35px !important;
            margin: -9px 0 -9px !important; /*To fill li background color*/
            padding: 7px 0 0 8px !important; /*To fill li background color*/
        }
        /*To set margin of dropdown menu from main menu*/
        
        /*Set Drop-down SubMenu Style*/
        .dropdown-submenu
        {
            position: relative;
        }
        .dropdown-submenu > .dropdown-menu
        {
            top: 0;
            left: 100%; /*margin-top: -6px;
        margin-left: -1px;
        -webkit-border-radius: 0 6px 6px 6px;
        -moz-border-radius: 0 6px 6px 6px;
        border-radius: 0 6px 6px 6px;*/
        }
        .dropdown-menu
        {
            width:172px !important;
        }
        .dropdown-submenu > .dropdown-menu > li
        {
            background-color: #000099 !important;
            background-image: none;
            background-repeat: repeat;
        }
        .dropdown-submenu:hover > .dropdown-menu
        {
            display: block;
        }
        .dropdown-submenu > a:after
        {
            display: block;
            content: " ";
            float: right;
            width: 0;
            height: 0;
            border-color: transparent;
            border-style: solid;
            border-width: 5px 0 5px 5px;
            border-left-color: inherit;
            margin-top: 5px;
        }
        .dropdown-submenu:hover > a:after
        {
            border-left-color: inherit;
        }
        .dropdown-submenu.pull-left
        {
            float: none;
        }
        .dropdown-submenu.pull-left > .dropdown-menu
        {
            left: -100%; /*margin-left: 10px;
        -webkit-border-radius: 6px 0 6px 6px;
        -moz-border-radius: 6px 0 6px 6px;
        border-radius: 6px 0 6px 6px;*/
        }
