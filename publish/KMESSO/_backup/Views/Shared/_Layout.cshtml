    <script type="text/javascript">

        $(function () {
            $('body').on('click', '.modal-link', function (e) {
                $(this).removeData('bs.modal');
                $("#GeneralContent").empty();
                $("body").css("padding-right", 0);
                e.preventDefault();
                $(this).attr('data-target', '#mmodal-container');
                $(this).attr('data-toggle', 'modal');

            });
            $('body').on('click', '.modal-close-btn', function () {
                $(this).removeData('bs.modal');
                $("#GeneralContent").empty();
                $('#mmodal-container').modal('hide');
                $('#modal-content').modal('hide');
                $("body").css("padding-right", "0px !important");
                return false;
            });
            $('approve-btn').on('click', '.modal-close-btn', function () {
                $('#mmodal-container').modal('hide');
                $('#modal-content').modal('hide');
                $("#GeneralContent").empty();
                $("body").css("padding-right", "0px !important");
                return true;
            });

            $('#mmodal-container').on('hidden.bs.modal', function () {
                $(this).removeData('bs.modal');
                $("#GeneralContent").empty();
                $(".sp-container").remove();
                $(".sp-palette-container").remove();
                $(this).removeData('bs.modal');
                $("body").css("padding-right", "0px !important");
            });
            $('#CancelModal').on('click', function () {
                $(this).removeData('bs.modal');
                $("#GeneralContent").empty();
                $("body").css("padding-right", "0px !important");
                return false;
            });
            $('#MessageModal').on('hide.bs.modal', function () {
                $(this).removeData('bs.modal');
                $("body").css('padding-right', -17);
            });
            $('#modal-content').on('shown.bs.modal', function () {
                $("body.modal-open").removeAttr("style");
                $("body").css('padding-right', 0);
            });
            $('#modal-content').on('shown', function () {
                $("body.modal-open").removeAttr("style");
                $("body").css('padding-right', 0);
            })
        });

        function InitilizeSaveEventOfAssignColor() {
            $("#btnAssignColorSave").off("click");
            $("#btnAssignColorSave").on('click', function () {
                //  $(".NewlyAddedModal").remove();
                bootbox.dialog({
                    size: 'large',
                    message: "Do you really want to save record?",
                    title: "Assign Color",
                    closeButton: false,
                    buttons: {
                        main: {
                            label: "OK",
                            className: "btn btnOk ",
                            callback: function () {
                                PerformInsertion();
                            }
                        },
                        danger: {
                            label: "Cancel",
                            className: "btn btnCancel ",
                            callback: function () {

                            }
                        }

                    }
                });
                var elem = document.createElement("img");
                elem.src = "../images/confirm.png";
                elem.style.cssText = ' margin-right:10px;';
                $('.bootbox-body').closest("div").prepend(elem);
                $(".btnStyle").removeClass("btn");
                $(".bootbox-body").addClass("dlg2");
                $(".btnNewdlg").closest("div").addClass("NewModalFotter");
                $(".modal-header").addClass("NewModal-header");
                $(".modal-title").addClass("NewModal-Title");
                $(".modal-title").css("font-weight", "bold");
                $(".modal-lg").addClass("NewlyAddedModal");

            });
        }

        function InitilizeSaveEventOfAddLabInfo() {
            $("#btnLabInfoSave").off("click");
            $("#btnLabInfoSave").on('click', function () {
                //  $(".NewlyAddedModal").remove();
                bootbox.dialog({
                    size: 'large',
                    message: "Do you really want to save record?",
                    title: "Lab Information",
                    closeButton: false,
                    buttons: {
                        main: {
                            label: "OK",
                            className: "btn btnOk",
                            callback: function () {
                                PerformInsertion();
                            }
                        },
                        danger: {
                            label: "Cancel",
                            className: "btn btnCancel ",
                            callback: function () {

                            }
                        }

                    }
                });
                var elem = document.createElement("img");
                elem.src = "../images/confirm.png";
                elem.style.cssText = ' margin-right:10px;';
                $('.bootbox-body').closest("div").prepend(elem);
                $(".btnStyle").removeClass("btn");
                $(".bootbox-body").addClass("dlg2");
                $(".btnNewdlg").closest("div").addClass("NewModalFotterAddLab");
                $(".modal-header").addClass("NewModal-headerAddLab");
                $(".modal-title").addClass("NewModal-TitleAddLab");
                $(".modal-title").css("font-weight", "bold");
                $(".modal-lg").addClass("NewlyAddedModalAddLab");

            });

        }
    </script>
